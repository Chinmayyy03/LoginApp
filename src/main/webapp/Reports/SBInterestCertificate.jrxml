	<queryString>
		<![CDATA[SELECT A.ACCOUNT_CODE,
       B.LEDGERBALANCE,
       TO_CHAR(A.DATEACCOUNTOPEN,'DD/MM/YYYY') DATEACCOUNTOPEN,
       NVL(SUM(D.AMOUNT),0) AMOUNT,
       A.NAME,
       NVL(TRIM(C.ADDRESS1),'') || ' ' ||
       NVL(TRIM(C.ADDRESS2),'') || ' ' ||
       NVL(TRIM(C.ADDRESS3),'') ADDRESS

FROM ACCOUNT.ACCOUNT A

JOIN BALANCE.ACCOUNT B
     ON B.ACCOUNT_CODE = A.ACCOUNT_CODE

LEFT JOIN CUSTOMER.CUSTOMER C
     ON A.CUSTOMER_ID = C.CUSTOMER_ID

LEFT JOIN TRANSACTION.DAILYTXN D
     ON D.ACCOUNT_CODE = A.ACCOUNT_CODE
     AND D.TXN_DATE BETWEEN
         TO_DATE($P{from_date},'YYYY-MM-DD')
         AND TO_DATE($P{to_date},'YYYY-MM-DD')
     AND D.TRANSACTIONINDICATOR_CODE LIKE '%CR'
     AND NVL(D.TRANIDENTIFICATION_ID,0)=0
     AND UPPER(D.PARTICULAR) LIKE '%INT%'

WHERE SUBSTR(A.ACCOUNT_CODE,1,4) = $P{branch_code}
  AND A.ACCOUNT_CODE = $P{account_code}

GROUP BY A.ACCOUNT_CODE,
         B.LEDGERBALANCE,
         A.NAME,
         C.ADDRESS1,
         C.ADDRESS2,
         C.ADDRESS3,
         A.DATEACCOUNTOPEN]]>
	</queryString>
