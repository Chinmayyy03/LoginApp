	<queryString>
		<![CDATA[SELECT A.SERIAL,
       B.TXN_OR_SCROLL_NO TXN_NUMBER,
       B.DEBITACCOUNT_CODE,
       B.CREDITACCOUNT_CODE,
       B.IS_EXECUTED,
       B.AMOUNT,
       B.DEBIT_PARTICULAR,
       B.CREDIT_PARTICULAR,
       B.USER_ID,
       B.OFFICER_ID,
       B.EXECUTDATE,
       B.SI_DATE,
       C.NAME AS CREDIT_NAME,
       D.NAME AS DEBIT_NAME
FROM ACCOUNT.STANDINGINSTRUCTIONS A,
     HISTORY.SIEXECUTEHISTORY B,
     ACCOUNT.ACCOUNT C,
     ACCOUNT.ACCOUNT D
WHERE
A.SERIAL = B.SERIAL
AND D.ACCOUNT_CODE = B.DEBITACCOUNT_CODE
AND C.ACCOUNT_CODE = B.CREDITACCOUNT_CODE
-- AND A.BRANCH_CODE = B.BRANCH_CODE
 AND A.BRANCH_CODE = $P{branch_code}
  AND A.SERIAL >= 1
  AND A.SERIAL <= 999999
  AND B.EXECUTDATE >= TO_DATE($P{as_on_date})
  AND B.EXECUTDATE <= TO_DATE($P{as_on_date})
ORDER BY A.SERIAL]]>
	</queryString>
