	<queryString>
		<![CDATA[SELECT SI.SERIAL,
       SI.DEBITACCOUNT_CODE,
       SI.CREDITACCOUNT_CODE,
       SI.AMOUNT,
       A.NAME AS DNAME,
       B.NAME AS CNAME,
       SI.SI_FREQUENCY,
       SI.STATUS,
       SI.START_DATE,
       SI.END_DATE,
       SI.NEXTDUE_DATE,
       SI.LASTEXECUTED_DATE
FROM ACCOUNT.STANDINGINSTRUCTIONS SI,
     ACCOUNT.ACCOUNT A,
     ACCOUNT.ACCOUNT B
WHERE SI.BRANCH_CODE = '0007'
  AND SI.DEBITACCOUNT_CODE = A.ACCOUNT_CODE
  AND SI.CREDITACCOUNT_CODE = B.ACCOUNT_CODE
  AND FN_GET_ACCOUNT_STATUS(SI.CREDITACCOUNT_CODE) = 'L'
ORDER BY SI.SERIAL]]>
	</queryString>
