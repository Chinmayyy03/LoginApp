	<queryString>
		<![CDATA[select rownum as sr,TXN_NUMBER,ACCOUNT_CODE,AMOUNT,NAME,USER_ID,OFFICER_ID from(
SELECT TXN_NUMBER,
              TRANSACTION.DAILYTXN.ACCOUNT_CODE,
              TRANSACTION.DAILYTXN.AMOUNT,
              INITCAP(trim(ACCOUNT.ACCOUNT.NAME)) AS NAME ,
              TRANSACTION.DAILYTXN.USER_ID,
              TRANSACTION.DAILYTXN.OFFICER_ID
            FROM TRANSACTION.TRANSACTION_HT_VIEW  DAILYTXN , ACCOUNT.ACCOUNT
              WHERE TRANSACTION.DAILYTXN.ACCOUNT_CODE = ACCOUNT.ACCOUNT.ACCOUNT_CODE
             --AND TRANSACTION.DAILYTXN.TRANSACTIONINDICATOR_CODE ='CSDR'
             AND TRANSACTION.DAILYTXN.TXN_NUMBER > 0
             --AND  TRANSACTION.DAILYTXN.BRANCH_CODE ='0002'
              --AND TRANSACTION.DAILYTXN.TXN_DATE = '29mar2025'
            --AND TRANSACTION.DAILYTXN.TRANSACTIONSTATUS='A'
 ORDER BY  TXN_NUMBER)]]>
	</queryString>
